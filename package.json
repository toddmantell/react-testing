{
  "name": "react-testing",
  "version": "0.1.0",
  "private": true,
  "prettier": {
    "printWidth": 100,
    "jsxBracketSameLine": true,
    "arrowParens": "avoid",
    "semi": false,
    "singleQuote": true
  },
  "jest": {
    "collectCoverageFrom": [
      "src/app/**/*.js"
    ]
  },
  "dependencies": {
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-scripts": "1.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --no-cache --coverage --verbose --env=jsdom",
    "eject": "react-scripts eject"
  },
  "devDependencies": {
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "jest-fetch-mock": "^1.6.5",
    "prettier": "^1.13.7"
  }
}
